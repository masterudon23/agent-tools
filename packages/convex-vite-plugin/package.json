{
  "name": "convex-vite-plugin",
  "version": "0.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/juliusmarminge/agent-tools",
    "directory": "packages/convex-vite-plugin"
  },
  "type": "module",
